{
  "name": "gatsby-source-openapi-aggregate",
  "version": "1.0.0",
  "scripts": {
    "build": "babel ./src --out-dir . --ignore __tests__",
    "watch": "babel ./src -w . --out-dir . --ignore __tests__",
    "test": "jest",
    "test:watch": "jest --verbose --watchAll ",
    "release": "standard-version"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      ".cache"
    ],
    "verbose": true
  },
  "dependencies": {
    "inspected": "0.4.0",
    "lodash.omit": "4.5.0",
    "openapi-parse": "^0.2.4",
    "ramda": "^0.25.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-stage-0": "6.24.1",
    "cross-env": "5.2.0",
    "jest": "23.6.0",
    "standard-version": "4.2.0"
  },
  "peerDependencies": {
    "gatsby": ">=1"
  }
}
