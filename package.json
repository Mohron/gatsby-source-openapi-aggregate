{
  "name": "gatsby-source-openapi-aggregate",
  "version": "0.1.7",
  "description": "Gatsby source plugin for pulling data into Gatsby from Open API/Swagger specifications",
  "scripts": {
    "build": "babel ./src --out-dir . --ignore __tests__",
    "watch": "babel ./src -w . --out-dir . --ignore __tests__",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "release": "standard-version"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      ".cache"
    ]
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-source-plugin"
  ],
  "author": "DevDigital <devdigital.cloud@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "babel-runtime": "6.26.0",
    "deepmerge": "^2.0.1",
    "spected": "^0.6.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "5.0.5",
    "jest": "^21.2.1",
    "standard-version": "4.2.0"
  }
}
