{
  "name": "gatsby-source-openapi-aggregate",
  "version": "0.1.8",
  "description": "Gatsby source plugin for pulling data into Gatsby from Open API/Swagger specifications",
  "scripts": {
    "build": "babel ./sample/plugins/gatsby-source-openapi-aggregate --out-dir . --ignore __tests__",
    "watch": "babel ./sample/plugins/gatsby-source-openapi-aggregate -w . --out-dir . --ignore __tests__",
    "test": "echo No tests specified",
    "release": "standard-version"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-source-plugin"
  ],
  "author": "DevDigital <devdigital.cloud@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "babel-runtime": "6.26.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "cross-env": "5.0.5",
    "standard-version": "4.2.0"
  }
}
